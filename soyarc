APNAME="soya"
APHOME=${HOME}
APPATH=${APHOME}/${APNAME}
APLOGD=${APHOME}/logs/${APNAME}

# Only two characters, to get idea of type of app on your screen -ls command
APTYPE="AP"

# activate log file
VIEWLOG=true

# log on screen to log file
APDEBUG=false
APDLINES=40

# commands to CMS's
APLOGSON="tracelog ALL"
APLOGSOFF="tracelog OFF"
APSYSLOGSON="systemlog ALL"
APSYSLOGSOFF="systemlog OFF"
APDBLOGSON="databaselog ALL"
APDBLOGSOFF="databaselog OFF"
APLOGSCLEAR=NONE
APBACKUP=NONE

